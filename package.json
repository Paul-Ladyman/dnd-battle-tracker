{
  "name": "dnd-battle-tracker",
  "version": "2.1.1",
  "private": true,
  "dependencies": {
    "fast-deep-equal": "^2.0.1",
    "file-saver": "^2.0.2",
    "is-hotkey": "^0.1.6",
    "jsonschema": "^1.2.5",
    "lodash.find": "^4.6.0",
    "lodash.findindex": "^4.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "scripts": {
    "start": "npm run build:index && webpack-dev-server --open --mode development",
    "build": "npm run build:index && npm run build:react && npm run build:bundle:html",
    "build:react": "webpack --mode production",
    "build:bundle:html": "node ./scripts/bundle-html.js",
    "build:index": "node ./scripts/create-index.js",
    "test": "npx jest --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/jest.init.js"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.1",
    "mini-css-extract-plugin": "^1.2.1",
    "mustache": "^3.2.0",
    "webpack": "^5.3.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
